
@{
    ViewData["Title"] = "PredictionSummary";
}

<h1>Prediction Summary</h1>

<div class="pie-chart">
    <canvas id="SummaryBudgetChart" width="300" height="300"></canvas>
</div>
<div>
    <h3>List of Employees</h3>
</div>

<div class="col-12 border p-3">
    <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr class="">
                <th>Employee ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Performance Rating</th>
                <th>Position</th>
                <th>Position Changed</th>
                <th>Salary</th>
                <th>Previous Salary</th>
                <th>Supervisor</th>
                <th>Team</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th>123456</th>
                <th>John</th>
                <th>Smith</th>
                <th>EC</th>
                <th>Director</th>
                <th>No</th>
                <th>$150,000</th>
                <th>$145,000</th>
                <th>Kate Owens</th>
                <th>Orange Team</th>
            </tr>
            <tr></tr>
        </tbody>
    </table>
</div>

<script src="~/js/PredictionsReport.js" asp-append-version="true"></script>